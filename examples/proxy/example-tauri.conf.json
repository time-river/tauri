{
  "$schema": "../../core/tauri-config-schema/schema.json",
  "build": {
    "distDir": [],
    "devPath": [],
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "package": {
    "productName": "Proxy",
    "version": "0.1.0"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "../.icons/32x32.png",
        "../.icons/128x128.png",
        "../.icons/128x128@2x.png",
        "../.icons/icon.icns",
        "../.icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "exceptionDomain": ""
      }
    },
    "windows": [
      {
        "title": "Proxy",
        "proxy": "socks5://127.0.0.1:1080",
        "url": "https://example.com",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src 'self'; connect-src ipc: http://ipc.localhost"
    }
  }
}
